<%= form_with(model: user) do |form| %>
  <% if user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
        <% user.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :Email %>
    <%= form.text_field :user_email %>
  </div>

  <div class="field">
    <%= form.label :Nome %>
    <%= form.text_field :user_first_name %>
  </div>

  <div class="field">
    <%= form.label :Sobrenome %>
    <%= form.text_field :user_last_name %>
  </div>

  <div class="field">
    <%= form.label :CPF %>
    <%= form.text_field :user_cpf %>
  </div>

  <div class="actions">
    <%= form.submit :Cadastrar%>
  </div>
<% end %>
