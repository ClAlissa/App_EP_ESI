<h1>Usu√°rios</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Nome</th>
      <th>Sobrenome</th>
      <th>CPF</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.user_email %></td>
        <td><%= user.user_first_name %></td>
        <td><%= user.user_last_name %></td>
        <td><%= user.user_cpf %></td>
        <td><%= link_to 'Visualizar', user %></td>
        <td><%= link_to 'Editar', edit_user_path(user) %></td>
        <td><%= button_to "Deletar", user, data: { confirm: "Are you sure?", disable_with: "Processing..." } , method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

